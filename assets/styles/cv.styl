.dropdown-menu li a,
.glyphicon-star,
.project-header,
.screenshot,
.participant-photo
  cursor pointer

.btn-add-tech, .btn-add-project
  display none
  left 130px
  opacity 0.4
  position absolute !important
  top 10px
  a
    color #333 !important
  &:hover
    opacity 1

.btn-add-project
  left 90px !important

.user-technologies
  &:hover
    .btn-add-tech
      display block

.user-projects
  &:hover
    .btn-add-project
      display block

#project-description
  resize vertical !important

.category-name, .project-label
  color #8D8D8D

.technologies-list
  list-style-type none
  padding-left 0 !important
  li
    margin-bottom 10px

.stars-1
  outline: 0
  border-radius 20px
  margin-left 5px
  background-color hsl(36, 100%, 79%) !important
  background-repeat repeat-x
  filter unquote("progid:DXImageTransform.Microsoft.gradient(startColorstr='#fffcf9', endColorstr='#ffd493')")
  background-image -khtml-gradient(linear, left top, left bottom, from(#fffcf9), to(#ffd493))
  background-image -moz-linear-gradient(top, #fffcf9, #ffd493)
  background-image -ms-linear-gradient(top, #fffcf9, #ffd493)
  background-image -webkit-gradient(linear, left top, left bottom, color-stop(0%, #fffcf9), color-stop(100%, #ffd493))
  background-image -webkit-linear-gradient(top, #fffcf9, #ffd493)
  background-image -o-linear-gradient(top, #fffcf9, #ffd493)
  background-image linear-gradient(#fffcf9, #ffd493)
  border-color #ffd493 #ffd493 hsl(36, 100%, 74%)
  color #333 !important
  text-shadow 0 1px 1px rgba(255, 255, 255, 0.33)
  -webkit-font-smoothing antialiased

